irmovl	$0, %eax		# 0 = sum
irmovl	$1, %ebx		# 1 = n
irmovl	$100, %ecx		# ecx = 100
irmovl	$100, %edx		# The address to store the printing word at
.Loop:
addl	%ebx, %eax		# sum += n
addl	$1, %ebx		# n += 1
cmpl	%ecx, %ebx		# Compare ecx and ebx
je	.Loop			# Jump back to Loop
rmmovl	%eax, $0(%edx)		# Move sum to the indicated memory address.
writel	$0(%edx)		# Write the value to console.
